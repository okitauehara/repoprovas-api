{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "linebreak-style": 0,
        "import/extensions": [
            "error",
            {
                "ts": "ignorePackages"
            }
        ],
        "import/no-unresolved": 0,
        "no-shadow": "off",
        "@typescript-eslint/no-shadow": [
            "error"
        ],
        "import/prefer-default-export": 0,
        "no-unused-vars": 0,
        "consistent-return": 0,
        "no-undef": 0
    }
}
